{
  "name": "baby-tracker",
  "version": "1.5.1",
  "description": "A simple website to track newborn baby events",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "test": "node tests/run-tests.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
